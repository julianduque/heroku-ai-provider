{
  "name": "heroku-ai-provider",
  "version": "0.0.1",
  "description": "Heroku Managed Inference and Agents provider for the Vercel AI SDK",
  "type": "module",
  "author": "Juli√°n Duque <jduque@heroku.com>",
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run fix-cjs",
    "fix-cjs": "node scripts/fix-cjs-extension.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "docs": "typedoc",
    "docs:serve": "typedoc --watch",
    "lint:staged": "lint-staged",
    "commitlint": "commitlint --edit",
    "prepare": "husky && pnpm run build"
  },
  "keywords": [
    "ai",
    "heroku",
    "vercel",
    "ai-sdk",
    "llm",
    "chat",
    "embeddings"
  ],
  "packageManager": "pnpm@10.12.1",
  "dependencies": {
    "@ai-sdk/provider": "^1.1.3",
    "@ai-sdk/provider-utils": "^2.2.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.29.0",
    "@eslint/markdown": "^6.5.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.1",
    "ai": "^4.3.16",
    "eslint": "^9.29.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.0",
    "typedoc": "^0.28.5",
    "typedoc-plugin-markdown": "^4.6.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0",
    "zod": "^3.25.64"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "tests/**/*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.config.{js,ts,cjs}": [
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/julianduque/heroku-ai-provider"
  },
  "bugs": {
    "url": "https://github.com/julianduque/heroku-ai-provider/issues"
  },
  "homepage": "https://github.com/julianduque/heroku-ai-provider#readme"
}
